generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Transaction {
  id        Int      @id @default(autoincrement())
  symbol    String
  type      String
  amount    Float
  price     Float
  total     Float
  date      DateTime
  createdAt DateTime @default(now())
}
